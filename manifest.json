{
    "name": "ButtonClicker",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", "https://*/*", "<all_urls>", "background"
      ]
}